{
  "name": "allidsbyid",
  "version": "1.2.2",
  "description": "Collection of functions to change objects that are in a allIds/byId shape without mutating them",
  "main": "./distribution/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "babel source --out-dir distribution --extensions \".ts\"",
    "buildTypes": "tsc -p tsconfigForDeclaration.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ka1/allidsbyid.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "typescript": "^3.4.2"
  },
  "author": "Kai Kasugai",
  "license": "UNLICENSED",
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/distribution"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "tsx",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "dependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0"
  }
}
